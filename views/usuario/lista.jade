div.row
  div.col-lg-12
   h1 Listagem de usuários
   
   div.panel.panel-default
    div.panel-heading 
     h3.panel-title Tabela de Usuários
    div.panel-body
     table.table.table-striped
      thead
       tr
        th ID:
        th Nome: 
        th Login: 
        th Senha:
        th Data:
        th Controle:
      tbody
       each valor, i in lista
        tr
         td #{valor.id}
         td #{valor.nome}
         td #{valor.login}
         td #{valor.senha}
         td #{valor.data_cad}
         td
          a(href="#" data-id="#{valor.id}").js-delete 
           i.fa.fa-times-circle